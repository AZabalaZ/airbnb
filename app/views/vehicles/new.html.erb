<h1>create a new vehicle</h1>
<div class="container">
<%= form_with model: @vehicle, local: true do |form| %>
  <div class="field">
    <%= form.label :make %>
    <%= form.text_field :make %>
  </div>
  <div class="field">
    <%= form.label :model %>
    <%= form.text_field :model %>
  </div>
  <div class="field">
    <%= form.label :year %>
    <%= form.text_field :year %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

</div>
<div><%= link_to 'Back', vehicles_path %></div>
<div><%= link_to 'Mis publicaciones', my_vehicles_path %></div>
